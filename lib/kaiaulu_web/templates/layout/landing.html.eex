    			}

    				#footer .copyright li:first-child {
    					border-left: 0;
    					margin-left: 0;
    					padding-left: 0;
    				}

    			#footer .copyright a {
    				color: inherit;
    			}

    		#footer:hover {
    			opacity: 1;
    		}

    		#footer > :last-child {
    			margin-bottom: 0;
    		}

    		@media screen and (max-width: 1680px) {

    			#footer {
    				bottom: 3.5em;
    				left: 3.5em;
    			}

    		}

    		@media screen and (max-width: 736px) {

    			#footer {
    				bottom: 2em;
    				left: 2em;
    			}

    		}

    		@media screen and (max-width: 360px) {

    			#footer {
    				bottom: 1.25em;
    				left: 1.25em;
    			}

    		}

    		@media screen and (max-height: 640px) {

    			#footer {
    				bottom: auto;
    				left: auto;
    				margin: 1em 0 0 0;
    				position: relative;
    			}

    		}

        .form-submit {
          font-size: 16pt;
        }

        #email {
          margin: 0 0 0 0;
        }


